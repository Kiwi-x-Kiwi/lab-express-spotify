      <img class="card-img" src="{{this.images.[0].url}}" alt="">
  </div>
  <div class="card-body">
    <h4 class="card-title">{{this.name}}</h4>
    <a href="/albums/{{this.id}}">View Albums</a>
    {{!-- <button type="button">View Albums</button> --}}
  </div>
</div>


